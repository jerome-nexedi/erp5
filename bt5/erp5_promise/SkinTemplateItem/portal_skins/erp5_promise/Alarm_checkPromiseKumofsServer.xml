        <item>
            <key> <string>_body</string> </key>
            <value> <string>from Products.CMFActivity.ActiveResult import ActiveResult\n
\n
portal = context.getPortalObject()\n
promise_url = portal.getPromiseParameter(\'external_service\', \'kumofs_url\')\n
\n
if promise_url is None:\n
  return\n
\n
plugin = portal.portal_memcached.restrictedTraverse("persistent_memcached_plugin", None)\n
if plugin is None:\n
  return\n
\n
url = "memcached://%s/" % plugin.getUrlString()\n
\n
active_result = ActiveResult()\n
\n
if promise_url != url:\n
  severity = 1\n
  summary = "Kumofs not configured as expected"\n
else:\n
  severity = 0\n
  summary = "Nothing to do."\n
\n
active_result.edit(\n
  summary=summary, \n
  severity=severity, \n
  detail="Expect %s\\nGot %s" % (promise_url, url))\n
\n
context.newActiveProcess().postResult(active_result)\n
</string> </value>
